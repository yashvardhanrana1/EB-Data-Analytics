## Basic print Program 

print("Enter your name : ")
name=input()
print("Enter your dep : ")
dep=input()
age= int(input("Enter your age : "))
print(name)
print(dep)
print(age)


## MONTH PROGRAM

month= int(input("Enter the month: "))
if month == 1:
    print("January")
elif month == 2:
    print("Februray")
elif month == 3:
    print("March")
elif month == 4:
    print("April")
elif month == 5:
    print("May")
elif month == 6:
    print("June")
elif month == 7:
    print("July")
elif month == 8:
    print("August")
elif month == 9:
    print("September")
elif month == 10:
    print("October")
elif month == 11:
    print("November")
elif month == 12:
    print("December")
else: 
    print("Invalid Input")
    
   
## Leap year or not 

year= int(input("Enter year : "))
if year % 4 == 0:
    print("Its a Leap Year")
else:
    print("Its not a Leap Year")
    
## print the Greater number

num1= int(input("Enter a Number : "))
num2= int(input("Enter sec. Number : "))
if num1>num2:
    print(num1, " is the Greater number")
else: 
    print(num2, " is the Greater number")
 
 
## eligible to Vote

age= int(input("Enter the age : "))
if age>18:
    print("You are eligible to Vote")
else:
    print("You are not eligible to Vote")


## Equal Numbers

num1 = int(input("Enter a number : "))
num2 = int(input("Enter sec number : "))
if num1 == num2:
    print("Both numbers are Equal")
else: 
    print("Number are not Equal")
    
 
## Increment on salary grater than 10000

salary = float(input("Enter your salary : "))
if salary>10000: 
    print("You are eligible for 10% bonus \nNew salary : ", ((salary/100)*10)+salary)
else: 
    print("You are not eligible for 10% bonus \n Salary : ", salary)


## Grading

marks = int(input("Enter student marks : "))
if marks>=91:
    print("Grade A")
elif marks>=76 and marks<=90:
    print("Grade B")
elif marks>=66 and marks<=75:
    print("Grade C")
elif marks>=50 and marks<=65: 
    print("Grade D")
else:
    print("Fail")
